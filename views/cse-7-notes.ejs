<% include partials/header%>
<div class="ui container">
<div class="ui cards">
  <% notes.forEach(function(note){%>
   <div class="card">
    <div class="content">
      <div class="header">
        <%= note.subName%>
      </div>
      <div class="meta">
        <%= note.branch.toUpperCase()%>
      </div>
    </div>
    <div class="extra content">
      <div class="ui basic blue button">
          <a href="<%=note.pdflink%>">Download <i class="cloud download icon"></i></a>
      </div>
    </div>
  </div>
 <%});%>
</div>
</div>
<% include partials/footer%>
